        <div class="detailsTitle">Details</div>
        <dl>
            <dt class="dt"> Location</dt>
            <dd class="dd"><%= myDocsId.place %> </dd>
            <dt class="dt">Number of Bed Room</dt>
            <dd class="dd"><%= myDocsId.numberOfBedRoom %> Bed Rooms</dd>
            <dt class="dt">Monthly Rent</dt>
            <dd class="dd"><%= myDocsId.monthlyrent %> Birr</dd>
            <dt class="dt">Floor</dt>
            <dd class="dd"><%= myDocsId.Flour %> Floor</dd>
            <dt class="dt">phonenumber</dt>
            <dd class="dd"><%=myDocsId.userphonenumber%></dd>
            <dt class="dt">User ID</dt>
            <dd class="dd"><%= myDocsId._id%></dd>
        </dl>
        <div>
            <form action="userApprovedMessage/<%= myDocsId._id%>" method="post">
                <button>Aprove!</button>
            </form>
        </div>
